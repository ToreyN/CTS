package CTS.booking;
import CTS.misc.*;

public class Ticket {
	private int ticketId;
	private Money pricePaid;
	
	
	public void setId(int ID) {
		ticketId = ID;
	}
	
	public void setPrice(Money pricePaid) {
		this.pricePaid = pricePaid;
	}
	
	public int getId() {
		return ticketId;
	}
	
	public Money getPrice() {
		return pricePaid;
	}
}
